+#pragma once
+
+/**
+ * Production Safety Module
+ * 
+ * This module provides runtime protections for the cpp_orderbook trading engine
+ * to ensure stability in production environments and prevent stack overflow
+ * scenarios that can crash the system during high-frequency trading.
+ */
+
+#include <atomic>
+#include <chrono>
+#include <stdexcept>
+#include <thread>
+
+class ProductionSafety {
+private:
+    static thread_local int recursion_depth;
+    static std::atomic<bool> safety_enabled;
+    static const int MAX_RECURSION_DEPTH = 50;
+    static const int STACK_CHECK_INTERVAL = 100;
+    
+    // Circuit breaker pattern for repeated failures
+    static std::atomic<int> failure_count;
+    static std::atomic<std::chrono::steady_clock::time_point> last_failure_time;
+    
+public:
+    // Enable/disable safety checks (runtime configurable)
+    static void enableSafety(bool enabled = true) {
+        safety_enabled.store(enabled);
+    }
+    
+    // Check recursion depth before entering critical operations
+    static bool enterCriticalOperation() {
+        if (!safety_enabled.load()) return true;
+        
+        int depth = ++recursion_depth;
+        if (depth > MAX_RECURSION_DEPTH) {
+            recursion_depth = 0; // Reset to prevent permanent lockout
+            return false; // Reject operation to prevent stack overflow
+        }
+        return true;
+    }
+    
+    // Exit critical operation
+    static void exitCriticalOperation() {
+        if (recursion_depth > 0) {
+            --recursion_depth;
+        }
+    }
+    
+    // Stack overflow protection using guard pattern
+    class StackGuard {
+    public:
+        StackGuard() : valid_(enterCriticalOperation()) {}
+        ~StackGuard() { 
+            if (valid_) exitCriticalOperation(); 
+        }
+        
+        bool isValid() const { return valid_; }
+        
+    private:
+        bool valid_;
+    };
+    
+    // Memory allocation safety
+    static void* safeAllocate(size_t size) {
+        if (!safety_enabled.load()) return malloc(size);
+        
+        // Prevent excessive allocations
+        if (size > 1024 * 1024) { // 1MB limit
+            return nullptr;
+        }
+        
+        return malloc(size);
+    }
+    
+    // Operation timeout protection
+    class TimeoutGuard {
+    public:
+        TimeoutGuard(std::chrono::milliseconds timeout = std::chrono::milliseconds(100))
+            : end_time_(std::chrono::steady_clock::now() + timeout) {}
+            
+        bool isExpired() const {
+            return std::chrono::steady_clock::now() > end_time_;
+        }
+        
+    private:
+        std::chrono::steady_clock::time_point end_time_;
+    };
+    
+    static bool circuitBreakerAllow() {
+        if (!safety_enabled.load()) return true;
+        
+        int failures = failure_count.load();
+        if (failures > 10) { // Too many failures
+            auto last_failure = last_failure_time.load();
+            auto now = std::chrono::steady_clock::now();
+            
+            // Reset after cooldown period
+            if (now - last_failure > std::chrono::seconds(30)) {
+                failure_count.store(0);
+                return true;
+            }
+            return false; // Circuit breaker is open
+        }
+        return true;
+    }
+    
+    static void recordFailure() {
+        failure_count.fetch_add(1);
+        last_failure_time.store(std::chrono::steady_clock::now());
+    }
+    
+    static void recordSuccess() {
+        if (failure_count.load() > 0) {
+            failure_count.fetch_sub(1);
+        }
+    }
+};
+
+// Thread-local member definition
+thread_local int ProductionSafety::recursion_depth = 0;
+
+// RAII macro for critical operations
+#define CRITICAL_OPERATION_GUARD \
+    ProductionSafety::StackGuard __stack_guard; \
+    if (!__stack_guard.isValid()) { \
+        ProductionSafety::recordFailure(); \
+        throw std::runtime_error("Operation rejected - recursion depth exceeded"); \
+    }
+
+// Circuit breaker macro
+#define CIRCUIT_BREAKER_GUARD \
+    if (!ProductionSafety::circuitBreakerAllow()) { \
+        throw std::runtime_error("Operation rejected - circuit breaker open"); \
+    }
